{
    "name": "kqm-lib",
    "version": "0.0.1",
    "scripts": {
        "dev": "not-svelte-kit dev",
        "build": "not-svelte-kit build",
        "build:vercel": "not-svelte-kit build && cp -R ./node_modules/@sveltejs/adapter-vercel/files ./node_modules/@sveltejs/files && not-svelte-kit adapt",
        "build:orig": "not-svelte-kit build",
        "adapt": "not-svelte-kit adapt",
        "start": "not-svelte-kit start",
        "clean": "rm -rf .svelte build functions api public .vercelignore vercel.json .vercel_build_output"
    },
    "devDependencies": {
        "@snowpack/plugin-build-script": "^2.1.0",
        "@snowpack/plugin-svelte": "^3.5.2",
        "@snowpack/plugin-typescript": "^1.0.0",
        "@sveltejs/adapter-begin": "^1.0.0-next.0",
        "@sveltejs/adapter-netlify": "^1.0.0-next.1",
        "@sveltejs/adapter-node": "next",
        "@sveltejs/adapter-static": "^1.0.0-next.2",
        "@sveltejs/adapter-vercel": "^1.0.0-next.2",
        "@sveltejs/snowpack-config": "next",
        "@types/node": "^14.14.31",
        "@types/node-fetch": "^2.5.8",
        "autoprefixer": "^10.2.4",
        "cssnano": "^4.1.10",
        "postcss": "^8.2.5",
        "postcss-cli": "^8.3.1",
        "postcss-load-config": "^3.0.1",
        "snowpack": "^3.0.11",
        "svelte": "^3.29.0",
        "svelte-preprocess": "^4.6.6",
        "tailwindcss": "^2.0.3",
        "tslib": "^2.0.0",
        "typescript": "^4.1.5"
    },
    "type": "module",
    "dependencies": {
        "not-svelte-kit": "0.0.7-next.34",
        "@sveltejs/kit": "next",
        "node-fetch": "^2.6.1"
    }
